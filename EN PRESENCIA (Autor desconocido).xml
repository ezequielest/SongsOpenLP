  <lyrics>
    <verse name="e1a">
      <lines>EN PRESENCIA DE<br/>JEHOVÁ<br/>PODEROSO DIOS<br/>PRÍNCIPE DE PAZ</lines>
    </verse>
    <verse name="e1b">
      <lines>LA AFLICCIÓN SE VA<br/>CORAZONES SON SANADOS<br/>EN PRESENCIA<br/>DE EL REY</lines>
    </verse>
  </lyrics>
