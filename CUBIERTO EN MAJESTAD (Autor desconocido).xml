  <lyrics>
    <verse name="v1">
      <lines>CUBIERTO EN MAJESTAD<br/>ÉL EN SU TRONO ESTÁ <br/>AL ALTO Y SUBLIME, QUIERO ADORAR</lines>
    </verse>
    <verse name="v2">
      <lines><br/>YO QUIERO ADORAR///<br/>EN ESPIRITU Y VERDAD</lines>
    </verse>
  </lyrics>
